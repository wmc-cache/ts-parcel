   // 如果是val对象,把val内部的属性转换成响应式对象\n    that.walk(val)\n    Object.defineProperty(obj, key, {\n      enumerable: true,\n      configurable: true,\n      get() {\n        // 收集依赖\n        Dep.target && dep.addSub(Dep.target)\n        console.log(Dep.target)\n        return val\n      },\n      set(newValue) {\n        if (newValue === val) {\n          return\n        }\n        val = newValue\n        that.walk(newValue)\n        // 发送通知\n        dep.notify()\n      },\n    })\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"835852e9c65154bdea26584c9c5462fd","cacheData":{"env":{}}}